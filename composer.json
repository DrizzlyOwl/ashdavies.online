{
    "name": "drizzlyowl/ashdavies.online",
    "description": "Ash's Website",
    "repositories": {
        "composer": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        "packagist.org": {
            "type": "composer",
            "url": "https://repo-eu-uk-1.packagist.org"
        }
    },
    "require": {
        "vlucas/phpdotenv": "^5.2",
        "deployer/dist": "^6.8",
        "composer/installers": "^1.9",
        "johnpbloch/wordpress": "^5.5",
        "wpackagist-plugin/wordpress-seo": "^15.3",
        "wpackagist-plugin/cloudflare": "^3.8"
    },
    "extra": {
        "wordpress-install-dir": "wordpress",
        "installer-paths": {
            "content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}
