{
    "name": "drizzlyowl/ashdavies.online",
    "description": "Ash's Website",
    "repositories": {
        "wpackagist": {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require": {
        "wpackagist-plugin/wordpress-seo": "^21",
        "wpackagist-plugin/theme-check": "^20230417",
        "wpackagist-plugin/redis-cache": "^2.0",
        "wpackagist-plugin/smtp-amazon-ses": "1.5"
    },
    "extra": {
        "installer-paths": {
            "app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}
